<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>

    </ui:style>
    <g:PopupPanel autoHideOnHistoryEventsEnabled="true" autoHideEnabled="true">
        <g:HTMLPanel>
            <table>
                <tr>
                    <td>
                        Type de Fichiers
                    </td>
                    <td align="center" width="50px">
                        <g:CheckBox ui:field="cbTypeALL" text="Tous"/>
                    </td>
                    <td align="center" width="50px">
                        <g:CheckBox ui:field="cbTypePDF" text="PDF"/>
                    </td>
                    <td align="center" width="50px">
                        <g:CheckBox ui:field="cbTypeDOCX" text="DOCX"/>
                    </td>
                    <td align="center" width="50px">
                        <g:CheckBox ui:field="cbTypeTXT" text="TXT"/>
                    </td>
                    <td align="center" width="50px">
                        <g:CheckBox ui:field="cbTypeXLS" text="XLS"/>
                    </td>
                </tr>
                <tr>
                    <td colspan="5" align="right">
                        <g:Button ui:field="btOK" text="OK"/>
                    </td>
                </tr>
            </table>
        </g:HTMLPanel>
    </g:PopupPanel>
</ui:UiBinder>
